algoritmo "exercicio_49"
var
cont, N , S : inteiro




inicio
      S <- 0
      Cont <- 1
      Enquanto (cont < 7) faca
               Escreva ("Digite",cont,"o. número: ")
               Leia (N)
               Cont <- cont + 1
               S <- S + N
      FimEnquanto
      Escreval (" A soma dos valores foi dos ",cont," valores é ",S,".")







fimalgoritmo