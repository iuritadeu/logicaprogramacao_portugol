algoritmo "exercicio_43"
var
N, N4: inteiro



inicio
      N <- 30
      Escreva (N)
      Enquanto (N > 1) faca
               N <- N - 1
               Se ( N % 4 = 0) entao
               Escreva ("{",N,"}")
               Senao
               Escreva (N)
               FimSe
      FimEnquanto


fimalgoritmo