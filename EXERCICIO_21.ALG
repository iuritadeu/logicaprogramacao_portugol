algoritmo "exercicio_21"

var
Ano, dias_fev:inteiro



inicio
      Escreva(" Digite o ano: ")
      Leia (Ano)
      Escreva ("Quantos dias em fevereiro? ")
      Leia (Dias_fev)
      Se (Dias_fev > 28) e (Dias_fev = 29)entao
         Escreval (Ano," é um ano bissexto!")
      Senao
           Escreval (Ano, " não é um ano bissexto!")
      FimSe



fimalgoritmo