algoritmo "exercicio_10"

var
l, a, mquad, ltinta: real

inicio
   Escreva("Largura da parede: ")
   Leia (l)
   Escreva ("Altura da parede: ")
   Leia (a)
   mquad <- l*a
   ltinta <- mquad/2
   Escreva (" O metro quadrado é ",mquad," o litro de tinta necessário é ",ltinta,"litros.")


fimalgoritmo